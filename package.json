{
    "name": "s-nomp",
    "version": "21.7.3",
    "description": "zero-proof (Verushash) node stratum mining pool based on NOMP",
    "keywords": [
        "stratum",
        "mining",
        "pool",
        "server",
        "poolserver",
        "equihash"
    ],
    "homepage": "https://github.com/ComputerGenieCo/s-nomp",
    "bugs": {
        "url": "https://github.com/ComputerGenieCo/s-nomp/issues"
    },
    "license": "MIT",
    "author": "ComputerGenieCo",
    "contributors": [
        "aayanl",
        "egyptianbman",
        "Matthew Little",
        "sennevb",
        "TheSeven",
        "s-nomp dev team",
        "vekexasia"
    ],
    "main": "init.js",
    "bin": {
        "block-notify": "./scripts/blockNotify.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ComputerGenieCo/s-nomp.git"
    },
    "dependencies": {
        "redis": "^3.0.0",
        "ecurve": "git+https://github.com/cryptocoinjs/ecurve.git#ee8a22ee3fcd78548a0b87c8d943b3ada4f5a26e"
    },
    "engines": {
        "node": ">20"
    },
    "gypfile": true,
    "scripts": {
        "build-verushash": "cd libs/verushash && node-gyp rebuild",
        "postinstall": "cd libs/verushash && node-gyp rebuild",
        "start": "NODE_PATH=$NODE_PATH:$PWD/libs/verushash/build/Release/ LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$PWD/libs/verushash/build/Release/ node init.js"
    }
}
